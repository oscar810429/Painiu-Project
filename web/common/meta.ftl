	<!-- HTTP 1.1 -->
	<meta http-equiv="Cache-Control" content="no-store,no-cache, must-revalidate,post-check=0, pre-check=0,max-age=0"/>
	<!-- HTTP 1.0 -->
	<meta http-equiv="Pragma" content="no-cache"/>
	<!-- Prevents caching at the Proxy Server -->
	<meta http-equiv="Expires" content="0"/>
	
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
	<meta name="title" content="${action.getText("page.meta.title")}"/>
	<meta name="description" content="${action.getText("page.meta.description")}"/>
	<meta name="keywords" content="${action.getText("page.meta.keywords")}"/>
	
	<link rel="shortcut icon" href="${media_root}/images/favicon.ico" type="image/ico"/>
	